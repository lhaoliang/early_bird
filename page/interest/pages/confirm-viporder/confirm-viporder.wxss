/* pages/confirm-order/confirm-order.wxss */

.container {
    padding: 20rpx 0 30rpx 0;
}

.person-info {
    padding: 30rpx 37rpx 0;
}

.person-info>image {
    width: 100%;
    height: 210rpx;
    display: block;
}

.inner {
    position: absolute;
    top: 105rpx;
    left: 65rpx;
}

.person,
.address {
    display: flex;
    justify-content: space-between;
    color: #fff;
    width: 580rpx;
    align-items: center;
}

.person {
    font-size: 28rpx;
    width: 520rpx;
    margin: 0 auto 30rpx;
}

.address {
    font-size: 24rpx;
    width: 620rpx;
}

.address>image {
    position: absolute;
    width: 54rpx;
    height: 36rpx;
    margin-left: 12rpx;
}

.address-car {
    display: flex;
    align-items: center;
    justify-content: center;
    color: #1cbdc5;
    font-size: 28rpx;
    border: dashed 2rpx #1cbdc5;
    width: 678rpx;
    height: 210rpx;
    margin: 0 auto;
    border-radius: 10rpx;
}

.address-car>image {
    width: 54rpx;
    height: 36rpx;
    margin-left: 12rpx;
}

