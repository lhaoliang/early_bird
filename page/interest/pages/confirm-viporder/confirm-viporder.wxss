.address-select {
    border-bottom: 10rpx solid #f0f5f5;
    padding-bottom: 38rpx;
}

.address-info {
    color: #323333;
    font-size: 28rpx;
    display: flex;
    justify-content: space-between;
    padding: 32rpx 92rpx 30rpx 92rpx;
}

.address-name {
    font-weight: bold;
}

.address-text {
    color: #646666;
    font-size: 24rpx;
    padding: 0 92rpx 0 48rpx;
    display: flex;
    align-items: center;
    word-break: break-all;
}

.address-text>image {
    width: 32rpx;
    height: 32rpx;
    margin-right: 12rpx;
    flex-shrink: 0;
}

.goods-info {
    padding: 0 36rpx 40rpx 36rpx;
    border-bottom: 10rpx solid #f0f5f5;
}

.goods-title {
    color: #323333;
    font-size: 28rpx;
    font-weight: bold;
    margin: 46rpx 0 26rpx 0;
}

.goods-options {
    border-bottom: 10rpx solid #f0f5f5;
}

.goods-option {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0 36rpx;
    height: 92rpx;
    font-size: 22rpx;
}

.goods-option:not(:last-of-type) {
    border-bottom: 1px solid #f0f5f5;
}

.option-key {
    color: #969999;
}

.option-value {
    color: #646666;

}

.option-value>span {
    font-size: 36rpx;
}

.option-value>text {
    font-size: 18rpx;
}

.option-value>image {
    opacity: 0.6;
    width: 30rpx;
    height: 30rpx;
    vertical-align: -5rpx;
}

.coupon {
    display: flex;
    justify-content: space-around;
}

.coupon-danger {
    font-size: 22rpx;
    color: #EE4D5D;
    margin-left: 330rpx;
    padding: 8rpx 20rpx;
    background-size: 100% 100%;
    background-image: url("data:image/png;base64,");
}

.coupon-primary {
    font-size: 22rpx;
    color: #1BC4C4;
    padding: 8rpx 20rpx;
    margin-left: 12rpx;
    background-size: 100% 100%;
    background-image: url("data:image/png;base64,");
}

input.option-value {
    flex-grow: 1;
}

.total-price {
    border-bottom: 1px solid #f0f5f5;
    padding: 26rpx 36rpx 12rpx 0;
    font-size: 24rpx;
    font-weight: bold;
    color: #323333;
    text-align: right;
}

.total-price>text {
    font-size: 36rpx;
}

.btn-primary {
    margin-top: 12rpx;
}

.icon-right>image {
    width: 26rpx;
    height: 26rpx;
    margin-right: 8rpx;
    vertical-align: middle;
}
.add-info{
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
}
.add-info>image {
    width: 22rpx;
    height: 26rpx;
    vertical-align: middle;
    margin-right: 27rpx;
}