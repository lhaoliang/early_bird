/* pages/interest/interest.wxss */


.container {
    padding-bottom: 50rpx;
    width: 100vw;
    background-color: #F0F5F5;
}

.card-container {
    border-radius: 10rpx 10rpx 0 0;
    background-color: #F0F5F5;
    /* position: absolute; */
    width: 680rpx;
    margin: 0 auto;
    /* top: 15%;
    left: 50%;
    margin-left: -340rpx; */
}


.top-bg {
    width: 100vw;
    height: 400rpx;
    z-index: -1;

}

.card-icon {
    height: 298rpx;
    background-color: #FFFFFF;
    padding: 40rpx;
    border-radius: 10rpx;
    position:absolute;
    top:75rpx;
    width:609rpx;
    left:30rpx;
}

.icon-title {
    color: #323333;
    font-size: 36rpx;
    display: flex;
    align-items: center;
    justify-content: flex-start;
    font-weight: bold;
}

.icon-title>image {
    margin-left: 12rpx;
    width: 28rpx;
    height: 28rpx;
}

.icons {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-top: 48rpx;


}

.icon-item {
    width: 170rpx;
    font-size: 24rpx;
    font-weight: 400;
    color: #323333;
    height: 184rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    align-items: center;

}

.icon-item>image {
    width: 122rpx;
    height: 122rpx;
}


.exchange-card {
    height: 134rpx;
    border-radius: 100rpx;
    background-color: #FEFFFE;
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-top: 78rpx;
    padding: 0 24rpx
}

.exchange-left {
    margin-left: 60rpx;

}

.exchange-title {
    color: #1BC4C4;
    font-size: 32rpx;
    font-weight: bold;
}

.exchange-content {
    font-weight: 400;
    font-size: 22rpx;
    color: #C8CCCC;
}

.exchange-right {
    width: 204rpx;
    height: 84rpx;
    font-weight: 500;
    font-size: 26rpx;
    color: #FFFFFF;
    display: flex;
    justify-content: center;
    align-items: center;
    background-image: url("data:image/png;base64,");
    background-size: 100% 100%;
}


.task-card {
    margin-top: 32rpx;
    width: 678rpx;

    background-color: #FFFFFF;
    border-radius: 10rpx;
    display: flex;
    justify-content: flex-start;
    flex-direction: column;
    align-items: center;
    padding: 40rpx 0 40rpx 0;
}

.task-title {
    color: #323333;
    font-size: 28rpx;
    margin:20rpx 0 40rpx 0;
    font-weight: bold;

}

.progress-title {
    color: #323333;
    font-size: 24rpx;
    font-weight: 700;
    margin: 20px 0 0 0;

}

.progress-item {
    background-color: #F2F7F7;
    width: 626rpx;
    height: 104rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    margin-top: 20rpx;


}

.progress-icon {
    width: 46rpx;
    height: 46rpx;
    margin: 0 18rpx;


}

.progress-text-title {
    color: #323333;
    font-size: 26rpx;
    font-weight: 700;

}

.progress-text-content {
    font-size: 22rpx;
    font-weight: 400;
    color: #646666;

}

.progress-item>button {

    font-size: 24rpx;
    font-weight: 400;
    color: #FFFFFF;
    width: 169rpx;
    height: 60rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    margin-right: 18rpx;
    border-radius: 0rpx;
}

.bgc-primary {
    background-color: #1BC4C4;
}

.bgc-danger {
    background-color: #EE4D5D;
}


.member-place-card {
    width: 678rpx;
    margin-top: 28rpx;
    background-color: #FFFFFF;
    border-radius: 10rpx;
    padding: 40rpx 0 40rpx 0;
}

.member-title {
    font-weight: 700;
    color: #323333;
    font-size: 36rpx;
    margin: 0 0 8rpx 28rpx;

}

.member-content {
    font-weight: 400;
    color: #646666;
    font-size: 24rpx;
    margin: 0 0 40rpx 28rpx;

}
