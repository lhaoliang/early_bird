/* pages/pay/pay.wxss */
.conner {
    padding: 0 36rpx;
}

.coupon {
    background-image: url("data:image/png;base64,");
    height: 320rpx;
    width: 100%;
    background-size: 100% 320rpx;
    display: flex;
    justify-content: center;
}

.coupon-info{
    text-align:center;

}

.cou-title {
    margin-top:50rpx;
    font-size: 28rpx;
    color: #fff;
    font-weight: 500;
}

.cou-price {
    color: #fff;
    margin-top: 20rpx;
    margin-bottom: 20rpx;

}

.cou-discount {
    height: 48rpx;
    line-height: 48rpx;
    color: #1CBDC5;
    font-size: 24rpx;
    padding: 0 28rpx;
    background-color: rgba(255, 255, 255, .7);
    border-radius: 48rpx;
    font-weight: 400;
}

.order-pad {
    padding: 20rpx 0;
}

.line {
    height: 10rpx;
    width: 100%;
    background: #EDF2F2;
}

.money-info {
    padding: 30rpx 36rpx;
}

.info-title {
    font-weight: bold;
    font-size: 28rpx;
    color: #323333;
    margin-bottom: 30rpx;
}

.info-item {
    color: #646666;
    font-size: 24rpx;
    line-height: 46rpx;
}

.pay-btn {
    position: fixed;
    width: 92%;
    left: 4%;
    bottom: 60rpx;
    color: #fff;
    font-size: 32rpx;
    text-align: center;
    height: 100rpx;
    line-height: 100rpx;
    background-color: #1CBDC5;
    border-radius: 5rpx;
}