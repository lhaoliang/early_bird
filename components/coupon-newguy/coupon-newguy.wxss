/* components/spec-modal/spec-modal.wxss */

.spec-modal {
    position: fixed;
    width: 100vw;
    height: 100vh;
    background: rgba(50, 51, 51, 0.3);
    left: 0;
    top: 0;
    z-index: 99;
}

.spec-body {
    height: 68vh;
    box-sizing: border-box;
    background-color: rgba(209, 202, 202, 0.4);
    margin: 10vh 53rpx;
    width: 646rpx;

    position: relative;
    box-shadow: 0 2rpx 30rpx 0 rgba(180, 179, 179, 0.2);
    animation: pop-up .3s;

}


@keyframes pop-up {
    from {
        margin-top: 100vh;
    }
}

.close-btn {
    position: absolute;
    right: 50%;
    top: 70vh;
    width: 60rpx;
    height: 60rpx;
    margin-right: -30rpx;

}

.coupon-box {
    height: 68vh;
    width: 626rpx;
    margin: 10rpx;
    position: absolute;
}

.title {
    display: flex;
    justify-content: center;
    align-items: center;
    width: 626rpx;
    height: 203rpx;

}

.member-title {
    color: #646666;
    font-size: 22rpx;
    font-weight: 400;
    text-align: center;
    width: 626rpx;
    padding-top: 20px;


}

.member-content {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-top: 48rpx;
    width: 552rpx;
    margin: 48rpx auto 60rpx;


}

.icon-item {
    width: 160rpx;
    font-size: 24rpx;
    font-weight: 400;
    color: #323333;
    height: 184rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    align-items: center;

}

.icon-item>image {
    width: 122rpx;
    height: 122rpx;
}

.bgc-danger {
    background: linear-gradient(90deg, rgba(238, 77, 126, 1) 0%, rgba(237, 77, 93, 1) 100%);
}

.bgc-primay {
    background: linear-gradient(90deg, rgba(22, 226, 236, 1) 0%, rgba(28, 189, 197, 1) 100%);
}

.title-top {
    font-size: 58rpx;
    font-weight: 600;
    color: rgba(255, 255, 255, 1);
    text-align: center;


}

.title-des {
    font-size: 28rpx;
    font-weight: 400;
    color: rgba(255, 255, 255, 1);
}

.content {
    background-color: #FFFFFF;
    height: 532rpx;



}

.content>image {
    width: 366rpx;
    height: 391rpx;
    margin: auto;
    position: relative;
    left: 50%;
    margin-left: -183rpx;
    top: 20rpx;
}

.content>.bot-btn {
    background-size: 100% auto;
    font-weight: 500;
    font-size: 32rpx;
    color: rgba(255, 255, 255, 1);
    width: 518rpx;
    height: 90rpx;
    /* border-radius: 100rpx; */
    /* background-color: #1cbdc5; */
    background-image:url('data:image/png;base64,');
    display: flex;
    justify-content: center;
    align-items: center;
    margin: 22rpx auto 10rpx;
    /* position: fixed;
    margin: auto;
    left: 110rpx;
    bottom:286rpx; */


}

.spec-top {
    box-sizing: border-box;
    padding: 70rpx 36rpx 44rpx 36rpx;
    border-bottom: 10rpx solid #f0f5f5;
    display: flex;
}

.spec-top>image {
    width: 230rpx;
    height: 230rpx;
    margin-right: 42rpx;
}

.price {
    color: #323333;
    font-size: 34rpx;
    font-weight: bold;
    margin-top: 16rpx;
}

.price>text {
    font-size: 26rpx;
}

.spec,
.stock {
    color: #646666;
    font-size: 24rpx;
    margin-top: 16rpx;
}

.spec-scroll {
    overflow: auto;
    height: calc(80vh - 486rpx);
}



.spec-list {
    padding: 0 36rpx;
    margin-left: -52rpx;
    display: flex;
    justify-content: flex-start;

}

.spec-item {
    border: solid 2rpx #969999;
    border-radius: 10rpx;
    padding: 12rpx 30rpx 12rpx 26rpx;
    color: #969999;
    font-size: 24rpx;
    display: inline-block;
    margin-left: 52rpx;
    margin-top: 36rpx;
    display: flex;
    justify-content: center;
    align-items: center;

}

.spec-item.active {
    border: solid 2rpx #1cbdc5;
    color: #1cbdc5;
}

.count-box {
    display: flex;
    align-items: center;
    padding: 30rpx 36rpx;
}

.count {
    color: #646666;
    font-size: 40rpx;
    margin: 0 10rpx;
    min-width: 66rpx;
    text-align: center;
}

.limit {
    color: #1cbdc5;
    font-size: 22rpx;
    margin-left: 46rpx;
}

button::after {
    border: none;
}

/* 底部大按钮 */

.spec-bottom {
    position: fixed;
    width: 100vw;
    bottom: 0;
    padding: 20rpx 0;
    background-color: #fff;
    border-top: 1px solid #f0f5f5;
}

.btn-primary {
    width: 678rpx;
    height: 100rpx;
    line-height: 100rpx;
    color: #fff;
    font-size: 32rpx;
    background-color: #1cbdc5;
    border-radius: 6rpx;
}